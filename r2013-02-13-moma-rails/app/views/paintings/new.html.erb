<h1>New Painting:</h1>
<%= form_for @painting do |f| %>
  <%= f.label :artist_id %><br>
  <%= f.select :artist_id, Artist.all.map{|artist| [artist.name, artist.id]}, :include_blank => true, :prompt => "Select Artist:" %><br>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :image %><br>
  <%= f.text_field :image %><br>
  <%= f.label :medium %><br>
  <%= f.text_field :medium%><br>
  <%= f.label :year %><br>
  <%= f.text_field :year%><br>
  <%= f.label :style %><br>
  <%= f.text_field :style %><br>
  <%= f.submit %>
<% end %>


